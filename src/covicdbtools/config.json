{
  "fields": {
    "ab_id": {
      "field": "ab_id",
      "label": "Antibody",
      "type": "id",
      "description": "the COVID ID for the antibody",
      "example": "COVIC:1",
      "comment": "blinded",
      "used in": "antibodies.tsv assays.tsv"
    },
    "ab_label": {
      "field": "ab_label",
      "label": "Antibody label",
      "type": "label",
      "description": "the COVIC label for the antibody",
      "example": "COVIC 1",
      "comment": "blinded",
      "used in": "antibodies.tsv"
    },
    "ab_name": {
      "field": "ab_name",
      "label": "Antibody name",
      "type": "label",
      "secret": "TRUE",
      "description": "the submitter's name for the antibody",
      "example": "VD-Crotty 1",
      "comment": "confidential submitter's preferred name",
      "used in": "secret/antibodies.tsv"
    },
    "ab_details": {
      "field": "ab_details",
      "label": "Antibody details",
      "type": "text",
      "secret": "TRUE",
      "description": "assay details about the antibody",
      "comment": "confidential, unstructured assay details on antibody from the sumbitter",
      "used in": "secret/antibodies.tsv"
    },
    "ab_comment": {
      "field": "ab_comment",
      "label": "Antibody comment",
      "type": "text",
      "secret": "TRUE",
      "description": "general comments about the antibody",
      "comment": "confidential free text about the antibody",
      "used in": "secret/antibodies.tsv"
    },
    "org_name": {
      "field": "org_name",
      "label": "Organization name",
      "type": "label",
      "secret": "TRUE",
      "description": "the name of the organization that submitted the antibody",
      "example": "LJI",
      "comment": "the organization that is submitted the antibody",
      "used in": "secret/antibodies.tsv"
    },
    "submitter_email": {
      "field": "submitter_email",
      "label": "Submitter email",
      "type": "email",
      "secret": "TRUE",
      "description": "the email of the person who submitted this data",
      "example": "shane@lji.org",
      "used in": "secret/antibodies.tsv secret/datasets.tsv"
    },
    "ds_id": {
      "field": "ds_id",
      "label": "Dataset",
      "type": "id",
      "description": "the ID of the dataset",
      "example": "ds:1",
      "used in": "secret/datasets.tsv dataset.yml"
    },
    "ds_label": {
      "field": "ds_label",
      "label": "Dataset label",
      "type": "label",
      "description": "the label of the dataset",
      "example": "Dataset 1",
      "used in": "dataset.yml"
    },
    "host_type_id": {
      "field": "host_type_id",
      "label": "Host",
      "type": "id",
      "terminology": "hosts",
      "description": "the ID of the host species",
      "example": "NCBITaxon:9606",
      "used in": "antibodies.tsv"
    },
    "host_type_label": {
      "field": "host_type_label",
      "label": "Host name",
      "type": "label",
      "description": "the label of the host species",
      "example": "Homo sapiens"
    },
    "isotype_id": {
      "field": "isotype_id",
      "label": "Isotype",
      "type": "id",
      "terminology": "isotypes",
      "description": "the ID of the antibody isotype",
      "example": "PR:000050177",
      "used in": "antibodies.tsv"
    },
    "isotype_label": {
      "field": "isotype_label",
      "label": "Isotype label",
      "type": "label",
      "description": "the label of the antibody isotype",
      "example": "IgA1"
    },
    "qualitative_measure": {
      "field": "qualitative_measure",
      "label": "Qualitiative measure",
      "type": "label",
      "terminology": "qualitative_measures",
      "description": "the qualitative measure of the assay",
      "example": "positive",
      "used in": "assays.tsv"
    },
    "titer": {
      "field": "titer",
      "label": "Titer",
      "type": "float",
      "description": "the titer value of the assay",
      "example": "0.52",
      "used in": "assays.tsv"
    },
    "comment": {
      "field": "comment",
      "label": "Comment",
      "type": "text",
      "description": "general comments about the assay",
      "example": "did not bind positive control",
      "used in": "assays.tsv"
    },
    "tested_antigen": {
      "field": "tested_antigen",
      "label": "Tested antigen",
      "type": "label",
      "description": "the name of the tested antigen",
      "example": "Spike protein 1",
      "used in": "assays.tsv"
    },
    "n": {
      "field": "n",
      "label": "n",
      "type": "int",
      "description": "the number of runs for the assay",
      "example": "6",
      "used in": "assays.tsv"
    }
  },
